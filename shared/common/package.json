{
  "name": "@unleashit/common",
  "version": "1.2.4",
  "description": "Shared library for @unleashit components. Not directly useful.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist/*.css": "./dist/*.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "*.png"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "dev": "NODE_ENV=development tsup --watch",
    "test": "NODE_ENV=test jest"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unleashit/npm-library.git"
  },
  "homepage": "https://github.com/unleashit/npm-library/tree/master/packages/common",
  "bugs": {
    "url": "https://github.com/unleashit/npm-library/issues"
  },
  "peerDependencies": {
    "@hookform/resolvers": "3.x",
    "react": "16.x || 17.x || 18.x",
    "react-hook-form": "7.x",
    "zod": "3.x"
  },
  "devDependencies": {
    "@unleashit/configs": "^1.0.5",
    "tsup": "^6.7.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "module": "dist/index.mjs",
  "gitHead": "c0e9afb314151ce1e9d37a00cc79fd7f0f22e184"
}
