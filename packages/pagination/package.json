{
  "name": "@unleashit/pagination",
  "version": "0.8.0",
  "main": "dist/index.js",
  "description": "Responsive pagination component for React.",
  "scripts": {
    "start": "NODE_ENV=development webpack --watch --source-maps --mode development",
    "build": "rm -rf ./dist && npm run sass:build && NODE_ENV=production webpack --mode production",
    "sass": "npm run sass:build && npm run sass:watch",
    "sass:build": "node-sass -r src/scss -o dist --source-map true",
    "sass:watch": "node-sass -wr src/scss -o dist --source-map true",
    "test": "NODE_ENV=test jest --config ./jest.config.js"
  },
  "license": "MIT",
  "keywords": [
    "pagination",
    "react pagination",
    "responsive",
