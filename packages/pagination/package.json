{
  "name": "@unleashit/pagination",
  "version": "0.0.4",
  "main": "dist/index.js",
  "description": "Responsive pagination component for React.",
  "scripts": {
    "start": "npm run build && NODE_ENV=development yarn webpack --watch --source-maps --mode development",
    "build": "rm -rf ./dist && NODE_ENV=production webpack --mode production",
    "test": "NODE_ENV=test jest"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unleashit/npm-library.git"
  },
  "homepage": "https://github.com/unleashit/npm-library/tree/master/packages/mockData",
  "bugs": {
    "url": "https://github.com/unleashit/npm-library/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=15.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@unleashit/mock-data": "^0.5.7",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@svgr/webpack": "^4.1.0",
    "babel-loader": "8.0.4",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "4.19.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/../../jestSetupReact.js",
    "testURL": "http://localhost/",
    "transform": {
      "\\.tsx?$": "ts-jest",
      "\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx, ts, tsx}"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/(config|dist|node_modules)/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/../../__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/../../__mocks__/styleMock.js"
    }
  },
  "gitHead": "af2f3b787aed55691b0141675f8f3d9a1aabac9c"
}
