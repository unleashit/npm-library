{
  "presets": ["@babel/preset-env", "@babel/preset-typescript", "@babel/preset-react"],
  "env": {
    "commonjs": {
      "plugins": [
        "@babel/transform-runtime",
        ["transform-react-remove-prop-types", { "mode": "wrap" }]
      ],
      "presets": ["@babel/preset-env", "@babel/preset-typescript", "@babel/preset-react"]
    },
    "esmodules": {
      "plugins": [
        "@babel/transform-runtime",
        ["transform-react-remove-prop-types", { "mode": "wrap" }]
      ],
      "presets": [
        ["@babel/preset-env", { "modules": false }],
        "@babel/preset-typescript",
        "@babel/preset-react"
      ]
    }
  }
}
