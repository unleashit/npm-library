{
  "name": "@unleashit/login",
  "version": "0.13.1",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "description": "Responsive login component for React with custom fields.",
  "scripts": {
    "start": "concurrently \"npm run sass\" \"npm run babel:watch\" \"npm run ts:watch\"",
    "babel:watch": "BABEL_ENV=esmodules babel src --out-dir dist/es --extensions \".ts,.tsx\" --watch --source-maps --verbose",
    "build": "rm -rf ./dist && concurrently \"npm run build:commonjs\" \"npm run build:es\" && concurrently \"npm run sass:build\" \"npm run ts\"",
    "build:commonjs": "BABEL_ENV=commonjs NODE_ENV=production babel src --out-dir dist/commonjs --extensions \".ts,.tsx\" --source-maps",
    "build:es": "BABEL_ENV=esmodules NODE_ENV=production babel src --out-dir dist/es --extensions \".ts,.tsx\" --source-maps",
    "ts": "concurrently \"npm run ts:commonjs\" \"npm run ts:es\"",
    "ts:commonjs": "tsc --declaration --outDir dist/commonjs --emitDeclarationOnly",
    "ts:es": "tsc --declaration --outDir dist/es --emitDeclarationOnly --module esnext",
    "ts:watch": "tsc --noEmit true --watch",
    "sass": "npm run sass:build && npm run sass:watch",
    "sass:build": "node-sass -r src/scss -o dist --source-map true",
    "sass:watch": "node-sass -wr src/scss -o dist --source-map true",
    "test": "NODE_ENV=test jest --config ./jest.config.js"
  },
  "license": "MIT",
  "keywords": [
    "login",
    "react login",
    "responsive",
    "custom fields",
    "react",
    "react components",
    "npm packages",
    "npm components",
    "ui components",
    "ui library",
    "monorepo",
    "lerna",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unleashit/npm-library.git"
  },
  "homepage": "https://github.com/unleashit/npm-library/tree/master/packages/login",
  "bugs": {
    "url": "https://github.com/unleashit/npm-library/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "formik": ">=1.0.0",
    "react": ">=15.0.0",
    "yup": ">=0.27.0"
  },
  "dependencies": {
    "@unleashit/common": "0.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/enzyme": "^3.10.1",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.135",
    "@types/react": "^16.8.22",
    "@types/yup": "^0.26.20",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-loader": "8.0.5",
    "concurrently": "^4.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "formik": "^1.5.7",
    "jest": "24.7.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react": "16.8.6",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "3.5.2"
  },
  "gitHead": "af2f3b787aed55691b0141675f8f3d9a1aabac9c"
}
