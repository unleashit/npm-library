{
  "name": "@unleashit/login",
  "version": "0.11.6",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "description": "Responsive login component for React with custom fields.",
  "scripts": {
    "start": "BABEL_ENV=esmodules babel src --out-dir dist/es --extensions \".ts,.tsx\" --watch --source-maps --verbose",
    "start:old": "NODE_ENV=development webpack --watch --source-maps --mode development",
    "build:old": "rm -rf ./dist && NODE_ENV=production webpack --mode production",
    "build": "rm -rf ./dist && npm run build:commonjs && npm run build:es",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir dist/commonjs --extensions \".ts,.tsx\" --source-maps",
    "build:es": "BABEL_ENV=esmodules babel src --out-dir dist/es --extensions \".ts,.tsx\" --source-maps",
    "test": "NODE_ENV=test jest --config ./jest.config.js"
  },
  "license": "MIT",
  "keywords": [
    "login",
    "react login",
    "responsive",
    "custom fields",
    "react",
    "react components",
    "npm packages",
    "npm components",
    "ui components",
    "ui library",
    "monorepo",
    "lerna",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unleashit/npm-library.git"
  },
  "homepage": "https://github.com/unleashit/npm-library/tree/master/packages/login",
