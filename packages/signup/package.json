{
  "name": "@unleashit/signup",
  "version": "1.1.8",
  "description": "Responsive signup component for React with custom fields.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist/*.css": "./dist/*.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "*.png"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "license": "MIT",
  "keywords": [
    "signup component",
    "registration component",
    "responsive",
    "custom fields",
    "react",
    "ui components",
    "ui library",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unleashit/npm-library.git"
  },
  "homepage": "https://github.com/unleashit/npm-library/tree/master/packages/signup",
  "bugs": {
    "url": "https://github.com/unleashit/npm-library/issues"
  },
  "peerDependencies": {
    "react": "16.x || 17.x || 18.x",
    "react-hook-form": "7.x",
    "zod": "3.x"
  },
  "dependencies": {
    "@unleashit/common": "workspace:*"
  },
  "module": "dist/index.mjs"
}
