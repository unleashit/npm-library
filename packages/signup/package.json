{
  "name": "@unleashit/signup",
  "version": "0.13.4",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "description": "Responsive signup component for React with custom fields.",
  "scripts": {
    "build": "NODE_ENV=production tsup && pnpm sass:build",
    "dev": "concurrently 'NODE_ENV=development tsup --watch' 'pnpm sass'",
    "sass": "pnpm sass:build && pnpm sass:watch",
    "sass:build": "node-sass -r src/scss -o dist --source-map true",
    "sass:watch": "node-sass -wr src/scss -o dist --source-map true",
    "test": "NODE_ENV=test jest"
  },
  "license": "MIT",
  "keywords": [
    "signup",
    "react signup",
    "registration",
    "react registration",
    "responsive",
    "custom fields",
    "react",
    "react components",
    "npm packages",
    "npm components",
    "ui components",
    "ui library",
    "monorepo",
    "lerna",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unleashit/npm-library.git"
  },
  "homepage": "https://github.com/unleashit/npm-library/tree/master/packages/signup",
  "bugs": {
    "url": "https://github.com/unleashit/npm-library/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "formik": "1.x || 2.x",
    "react": "16.x || 17.x || 18.x",
    "yup": "^0.32.11"
  },
  "dependencies": {
    "@unleashit/common": "workspace:*",
    "@unleashit/configs": "workspace:*"
  }
}
