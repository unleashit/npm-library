{
  "name": "@unleashit/modal",
  "version": "0.2.16",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "description": "Responsive modal component for React with custom header, footer and more.",
  "scripts": {
    "build": "NODE_ENV=production tsup && pnpm sass:build",
    "dev": "concurrently 'NODE_ENV=development tsup --watch' 'pnpm sass'",
    "sass": "pnpm sass:build && pnpm sass:watch",
    "sass:build": "node-sass -r src/scss -o dist --source-map true",
    "sass:watch": "node-sass -wr src/scss -o dist --source-map true",
    "test": "NODE_ENV=test jest"
  },
  "license": "MIT",
  "keywords": [
    "modal",
    "react modal",
    "responsive",
    "custom fields",
    "react",
    "react components",
    "npm packages",
    "npm components",
    "ui components",
    "ui library",
    "monorepo",
    "lerna",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unleashit/npm-library.git"
  },
  "homepage": "https://github.com/unleashit/npm-library/tree/master/packages/modal",
  "bugs": {
    "url": "https://github.com/unleashit/npm-library/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "16.x || 17.x || 18.x"
  },
  "dependencies": {
    "@unleashit/common": "workspace:*"
  },
  "devDependencies": {
    "@unleashit/common": "workspace:*"
  }
}
