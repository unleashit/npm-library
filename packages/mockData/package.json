{
  "name": "@unleashit/mock-data",
  "version": "1.0.1",
  "description": "Wrapper for mocker-data-generator providing a set of templates and arguments. Can optionally write to a file (node)",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      }
    },
    "access": "public"
  },
  "files": ["dist", "*.png"],
  "sideEffects": false,
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "dev": "NODE_ENV=development tsup --watch",
    "test": "NODE_ENV=test jest"
  },
  "keywords": [
    "mock data",
    "mock data templates",
    "faker",
    "chance",
    "casual",
    "npm",
    "typescript"
  ],
  "author": "unleashit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unleashit/npm-library.git"
  },
  "homepage": "https://github.com/unleashit/npm-library/tree/master/packages/mockData",
  "bugs": {
    "url": "https://github.com/unleashit/npm-library/issues"
  },
  "dependencies": {
    "mocker-data-generator": "^2.12.0"
  },
  "devDependencies": {
    "@unleashit/configs": "workspace:*"
  },
  "moduleDirectories": [
    "node_modules"
  ]
}
