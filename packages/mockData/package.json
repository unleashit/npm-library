{
  "name": "@unleashit/mock-data",
  "version": "0.7.1",
  "description": "Wrapper for mocker-data-generator providing a set of templates and arguments. Can optionally write to a file (node)",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "start": "npm run babel:watch & npm run ts:watch",
    "babel:watch": "BABEL_ENV=esmodules babel src --out-dir dist/es --extensions \".ts,.tsx\" --watch --source-maps --verbose",
    "build": "rm -rf ./dist && npm run build:commonjs && npm run build:es && npm run ts",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir dist/commonjs --extensions \".ts,.tsx\" --source-maps",
    "build:es": "BABEL_ENV=esmodules babel src --out-dir dist/es --extensions \".ts,.tsx\" --source-maps",
    "ts": "npm run ts:commonjs && npm run ts:es",
    "ts:commonjs": "tsc --declaration --outDir dist/commonjs --emitDeclarationOnly",
    "ts:es": "tsc --declaration --outDir dist/es --emitDeclarationOnly --module esnext",
    "ts:watch": "tsc --noEmit true --watch",
    "test": "NODE_ENV=test jest"
  },
  "keywords": [
    "mock",
    "mock data",
    "generator",
    "json",
    "mocker-data-generator",
    "faker",
    "chance",
    "casual",
    "randexp",
    "templates",
    "schema",
    "npm packages",
    "npm components",
    "ui components",
    "ui library",
    "monorepo",
    "lerna",
    "typescript"
  ],
  "author": "unleashit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unleashit/npm-library.git"
  },
  "homepage": "https://github.com/unleashit/npm-library/tree/master/packages/mockData",
  "bugs": {
    "url": "https://github.com/unleashit/npm-library/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "mocker-data-generator": "^2.6.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "jest": "24.7.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "3.5.2"
  },
  "jest": {
    "globalSetup": "./jest.config.js",
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "moduleDirectories": [
    "node_modules"
  ],
  "gitHead": "af2f3b787aed55691b0141675f8f3d9a1aabac9c"
}
