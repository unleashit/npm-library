{
  "name": "@unleashit/mock-data",
  "version": "0.6.5",
  "description": "Wrapper for mocker-data-generator providing a set of templates and arguments. Can optionally write to a file (node)",
  "main": "dist/index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack --watch --source-maps --mode development",
    "build": "rm -rf ./dist && NODE_ENV=production webpack --mode production",
    "build:old": "rm -rf ./dist && NODE_ENV=production tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint --ext .ts,.tsx,.js src --fix"
  },
  "keywords": [
    "mock",
    "mock data",
    "generator",
    "json",
    "mocker-data-generator",
    "faker",
    "chance",
    "casual",
    "randexp",
    "templates",
    "schema",
    "npm packages",
    "npm components",
    "ui components",
    "ui library",
    "monorepo",
    "lerna",
    "typescript"
  ],
  "author": "unleashit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unleashit/npm-library.git"
  },
  "homepage": "https://github.com/unleashit/npm-library/tree/master/packages/mockData",
  "bugs": {
    "url": "https://github.com/unleashit/npm-library/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "mocker-data-generator": "^2.6.6"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.6",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "23.6.0",
    "prettier": "^1.16.4",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "typescript": "3.3.4000",
    "uglifyjs-webpack-plugin": "^2.1.2"
  },
  "jest": {
    "globalSetup": "./jest.config.js",
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "moduleDirectories": [
    "node_modules"
  ],
  "gitHead": "af2f3b787aed55691b0141675f8f3d9a1aabac9c"
}
